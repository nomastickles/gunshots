{
  "name": "gunshots-backend",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "deploy": "yarn test && node_modules/.bin/serverless deploy --verbose",
    "destroy": "node_modules/.bin/serverless remove --verbose",
    "serverless:metrics": "node_modules/.bin/serverless metrics",
    "serverless:doctor": "node_modules/.bin/serverless doctor",
    "test": "jest",
    "test:watch": "yarn test -- --watch",
    "scripts:upload": "ARN=$ARN node scripts/uploadCSVwithSNS.js"
  },
  "resolutions": {
    "json5": "2.2.2",
    "qs": "6.9.7"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.241.0",
    "@middy/core": "^3.1.1",
    "@middy/http-json-body-parser": "^2.5.7",
    "@middy/ssm": "^3.1.1",
    "node-fetch": "2.6.7",
    "papaparse": "^5.3.2"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.25.0",
    "@types/aws-lambda": "^8.10.109",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@types/node-fetch": "^2.6.2",
    "aws-sdk": "^2.1286.0",
    "jest": "^29.3.1",
    "serverless": "3.26.0",
    "serverless-iam-roles-per-function": "^3.2.0",
    "serverless-stack-output": "^0.2.3",
    "serverless-webpack": "^5.11.0",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-node-externals": "^3.0.0"
