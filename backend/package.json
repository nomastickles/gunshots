{
  "name": "gunshots-backend",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "deploy": "yarn test && node_modules/.bin/serverless deploy --verbose --stage $STAGE",
    "destroy": "node_modules/.bin/serverless remove --stage $STAGE",
    "serverless:metrics": "node_modules/.bin/serverless metrics",
    "serverless:info": "node_modules/.bin/serverless info",
    "test": "jest",
    "test:watch": "yarn test -- --watch",
    "scripts:upload": "ARN=$ARN node scripts/uploadCSVwithSNS.js"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.142.0",
    "@middy/core": "^3.1.1",
    "@middy/http-json-body-parser": "^2.5.7",
    "@middy/ssm": "^3.1.1",
    "node-fetch": "2.6.7",
    "papaparse": "^5.3.2"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.21.0",
    "@types/aws-lambda": "^8.10.101",
    "@types/jest": "^29.1.2",
    "@types/node": "^18.6.4",
    "@types/node-fetch": "^2.6.2",
    "aws-sdk": "^2.1189.0",
    "jest": "^29.1.2",
    "serverless": "3.21.0",
    "serverless-iam-roles-per-function": "^3.2.0",
    "serverless-stack-output": "^0.2.3",
    "serverless-webpack": "^5.8.0",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-node-externals": "^3.0.0"
  },
  "author": "nomastickles",
  "license": "MIT"
}
